<Window x:Class="Yelp_Dataset_Challenge.User"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Yelp_Dataset_Challenge"
        mc:Ignorable="d"
        Title="User" Height="325.55" Width="300">
    <Grid>
        <ListBox x:Name="usersListBox" HorizontalAlignment="Left" Height="247" Margin="10,38,0,0" VerticalAlignment="Top" Width="272" SelectionChanged="friendsListBox_SelectionChanged"/>
        <TextBox x:Name="searchBox" HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="192"/>
        <Button x:Name="searchButton" Content="search" HorizontalAlignment="Left" Margin="207,10,0,0" VerticalAlignment="Top" Width="75" Height="23" Click="searchButton_Click"/>

    </Grid>
</Window>
