<Window x:Class="Yelp_Dataset_Challenge.userDetails"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Yelp_Dataset_Challenge"
        mc:Ignorable="d"
        Title="userDetails" Height="300" Width="300">
    <Grid>
        <Label x:Name="nameLabel" Content="Label" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Label x:Name="reviewStarsFansLabel" Content="Label" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top"/>
        <Label x:Name="votesLabel" Content="Label" HorizontalAlignment="Left" Margin="10,72,0,0" VerticalAlignment="Top"/>
        <Label x:Name="dateLabel" Content="Label" HorizontalAlignment="Left" Margin="10,103,0,0" VerticalAlignment="Top"/>
        <ListBox x:Name="friendList" HorizontalAlignment="Left" Height="94" Margin="10,165,0,0" VerticalAlignment="Top" Width="272" SelectionChanged="friendList_SelectionChanged"/>
        <Label x:Name="label" Content="friends :" HorizontalAlignment="Left" Margin="10,134,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
